{
  "name": "uni-app-network",
  "version": "0.10.2",
  "description": "为 uni-app 打造的基于 Promise 的 HTTP 客户端。",
  "keywords": [
    "uni-app",
    "uniapp",
    "request"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ModyQyW/uni-helper/tree/main/packages/uni-app-network"
  },
  "license": "MIT",
  "author": {
    "name": "ModyQyW",
    "email": "wurui-dev@foxmail.com",
    "url": "https://modyqyw.github.io"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": "./dist/*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup-node",
    "dev": "pnpm run build --watch src",
    "release": "npx release-it"
  },
  "dependencies": {
    "@dcloudio/types": "3.0.15",
    "@types/qs": "^6.9.7",
    "@types/ramda": "^0.28.18",
    "@types/statuses": "^2.0.0",
    "qs": "^6.11.0",
    "ramda": "^0.28.0",
    "statuses": "^2.0.1"
  },
  "devDependencies": {
    "@types/ungap__url-search-params": "^0.1.0",
    "@ungap/url-search-params": "^0.2.2"
  },
  "engines": {
    "node": ">=14.16"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): uni-app-network@v${version}",
      "tagName": "uni-app-network@v${version}"
    },
    "hooks": {
      "before:init": "pnpm install",
      "after:bump": "pnpm run build && cp -af README.md ../docs/docs/network/uni-app-network.md && pnpm publish --no-git-checks"
    },
    "npm": {
      "publish": false,
      "timeout": 30
    }
  }
}
