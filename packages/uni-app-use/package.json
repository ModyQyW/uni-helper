{
  "name": "uni-app-use",
  "version": "0.5.0",
  "description": "uni-app (vue3) 组合式工具集。",
  "keywords": [
    "uni-app",
    "uniapp",
    "uni",
    "use",
    "composition"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ModyQyW/uni-helper/tree/main/packages/uni-app-use"
  },
  "license": "MIT",
  "author": {
    "name": "ModyQyW",
    "email": "wurui-dev@foxmail.com",
    "url": "https://modyqyw.github.io"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": "./dist/*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup-node",
    "check:types": "npx tsc --noEmit",
    "dev": "pnpm run build --watch src",
    "lint": "run-p lint:eslint check:types",
    "lint:eslint": "npx eslint src --fix",
    "release": "npx release-it"
  },
  "dependencies": {
    "@dcloudio/types": "^3.0.13",
    "@dcloudio/uni-app": "^3.0.0-alpha-3050420220804008",
    "@vueuse/core": "^9.1.1"
  },
  "devDependencies": {
    "@swc/core": "^1.2.242",
    "@tsconfig/node16": "^1.0.3",
    "@types/node": "^18.7.11",
    "eslint": "^8.22.0",
    "npm-run-all": "^4.1.5",
    "tsup": "^6.2.2",
    "typescript": "^4.7.4",
    "vue": "^3.2.37"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): uni-app-use@v${version}",
      "tagName": "uni-app-use@v${version}"
    },
    "hooks": {
      "before:init": "pnpm install && pnpm run lint",
      "after:bump": "pnpm run build && cp -af README.md ../docs/docs/composition/uni-app-use.md"
    }
  }
}
