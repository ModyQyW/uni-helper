{
  "name": "uni-app-use",
  "version": "0.2.0",
  "description": "uni-app 的 vueuse，组合式工具集。",
  "keywords": [
    "uni-app",
    "uniapp",
    "uni",
    "use",
    "composition"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ModyQyW/uni-helper/tree/main/packages/uni-app-use"
  },
  "license": "MIT",
  "author": {
    "name": "ModyQyW",
    "email": "wurui-dev@foxmail.com",
    "url": "https://modyqyw.github.io"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": "./dist/*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup-node",
    "check:types": "npx tsc --noEmit",
    "dev": "pnpm run build --watch src",
    "lint": "run-p lint:eslint check:types",
    "lint:eslint": "npx eslint src --fix",
    "release": "npx release-it"
  },
  "dependencies": {
    "@dcloudio/types": "^2.6.12",
    "@dcloudio/uni-app": "^3.0.0-alpha-3050420220804003",
    "@vue/composition-api": "^1.7.0",
    "@vueuse/core": "^9.0.2",
    "@vueuse/shared": "^9.0.2",
    "uni-composition-api": "^0.6.1"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/node": "^18.6.3",
    "eslint": "^8.21.0",
    "npm-run-all": "^4.1.5",
    "tsup": "^6.2.1",
    "typescript": "^4.7.4",
    "vue": "^3.2.37"
  },
  "peerDependencies": {
    "vue": "^2.6.0 || ^2.7.0 || ^3.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): uni-app-use@${version}",
      "tagName": "uni-app-use@${version}"
    },
    "hooks": {
      "before:init": "pnpm install && pnpm run lint",
      "after:bump": "pnpm run build"
    }
  }
}
