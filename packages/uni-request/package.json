{
  "name": "@modyqyw/uni-request",
  "version": "0.4.6",
  "description": "`Promise` based HTTP client for `uni-app`. 为 `uni-app` 打造的基于 `Promise` 的 HTTP 客户端。",
  "keywords": [
    "uni-app",
    "uniapp",
    "request"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ModyQyW/uni-helper/tree/main/packages/uni-request"
  },
  "license": "MIT",
  "author": {
    "name": "ModyQyW",
    "email": "wurui-dev@foxmail.com",
    "url": "https://modyqyw.github.io"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": "./dist/*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup-node",
    "dev": "pnpm run build --watch src",
    "lint": "npx eslint src --fix && npx tsc --noEmit && vitest run --coverage",
    "release": "npx release-it",
    "test": "vitest --ui",
    "test:coverage": "vitest --ui --coverage"
  },
  "dependencies": {
    "@dcloudio/types": "^2.6.12",
    "@types/lodash-es": "^4.17.6",
    "@types/qs": "^6.9.7",
    "@types/ramda": "^0.28.15",
    "@types/statuses": "^2.0.0",
    "lodash-es": "^4.17.21",
    "qs": "^6.11.0",
    "ramda": "^0.28.0",
    "statuses": "^2.0.1"
  },
  "devDependencies": {
    "@swc/core": "^1.2.222",
    "@tsconfig/node16": "^1.0.3",
    "@types/node": "^18.6.3",
    "@types/ungap__url-search-params": "^0.1.0",
    "@ungap/url-search-params": "^0.2.2",
    "@vitest/ui": "^0.20.3",
    "c8": "^7.12.0",
    "tsup": "^6.2.1",
    "vitest": "^0.20.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): @modyqyw/uni-request@${version}",
      "tagName": "@modyqyw/uni-request@${version}"
    },
    "hooks": {
      "before:init": "pnpm install && pnpm run lint",
      "after:bump": "pnpm run build"
    }
  }
}
