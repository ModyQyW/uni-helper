{
  "extends": "@tsconfig/node16/tsconfig.json",
  "include": [
    "**/.*.js",
    "**/.*.cjs",
    "**/.*.mjs",
    "**/.*.ts",
    "**/.*.cts",
    "**/.*.mts",
    "**/*.js",
    "**/*.cjs",
    "**/*.mjs",
    "**/*.ts",
    "**/*.cts",
    "**/*.mts",
    "**/*.jsx",
    "**/*.tsx",
    "**/*.vue"
  ],
  "exclude": ["node_modules"]
}
